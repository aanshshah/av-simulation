    "        print(f\"Max acceleration: {ego_df['acceleration'].max():.2f} m/s¬≤\")\n",
    "        print(f\"Max deceleration: {ego_df['acceleration'].min():.2f} m/s¬≤\")\n",
    "\n",
    "# Run trajectory analysis\n",
    "analyze_trajectories(analysis_data)"
   ]
