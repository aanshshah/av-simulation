   "source": [
    "## üéÆ Simulation Runner Class"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "id": "runner_class"
   },
   "outputs": [],
   "source": [
    "import pygame\n",
    "from PIL import Image\n",
    "\n",
    "class AdvancedSimulationRunner:\n",
    "    \"\"\"Advanced simulation runner with comprehensive data collection\"\"\"\n",
    "    \n",
    "    def __init__(self, data_dir=\"simulation_runs\"):\n",
    "        self.data_dir = data_dir\n",
    "        os.makedirs(data_dir, exist_ok=True)\n",
    "        \n",
    "        self.repository = DataRepository(data_dir)\n",
    "        self.screenshots = []\n",
    "        self.metrics = {\n",
    "            'total_distance': 0,\n",
    "            'max_speed': 0,\n",
    "            'avg_speed': 0,\n",
    "            'collisions': 0,\n",
    "            'lane_changes': 0,\n",
    "            'simulation_time': 0\n",
    "        }\n",
    "        \n",
    "    def run_environment(self, env_type=\"highway\", duration=60, auto_pilot=True):\n",
    "        \"\"\"Run simulation in specified environment\"\"\"\n",
    "        from av_simulation.core.simulation import Simulation\n",
    "        \n",
    "        print(f\"üöÄ Starting {env_type} simulation for {duration}s...\")\n",
    "        \n",
    "        # Create simulation with data collection\n",
    "        sim = Simulation(enable_data_collection=True)\n",
    "        \n",
    "        # Switch to desired environment\n",
    "        if env_type in sim.environments:\n",
    "            sim.switch_environment(env_type)\n",
    "        \n",
    "        start_time = time.time()\n",
    "        last_screenshot = 0\n",
    "        screenshot_interval = max(2.0, duration / 20)  # Capture ~20 screenshots\n",
    "        \n",
    "        frame_count = 0\n",
    "        speed_samples = []\n",
    "        \n",
    "        try:\n",
    "            while time.time() - start_time < duration:\n",
    "                current_time = time.time() - start_time\n",
    "                \n",
    "                # Handle pygame events\n",
    "                for event in pygame.event.get():\n",
    "                    if event.type == pygame.QUIT:\n",
    "                        break\n",
    "                \n",
    "                # AI control or manual\n",
    "                if auto_pilot and sim.current_env.ego_vehicle:\n",
    "                    action = sim.planner.plan_action(sim.current_env.ego_vehicle)\n",
    "                    sim.current_env.ego_vehicle.set_action(action)\n",
    "                    \n",
    "                    # Collect metrics\n",
    "                    ego = sim.current_env.ego_vehicle\n",
    "                    speed = np.sqrt(ego.state.vx**2 + ego.state.vy**2)\n",
    "                    speed_samples.append(speed)\n",
    "                    self.metrics['max_speed'] = max(self.metrics['max_speed'], speed)\n",
    "                \n",
    "                # Update simulation\n",
    "                sim.current_env.step(1.0/60)\n",
    "                \n",
    "                # Collect data\n",
    "                if sim.data_collection_enabled:\n",
    "                    sim.collect_simulation_data()\n",
    "                \n",
    "                # Track collisions\n",
    "                if sim.current_env.collision_detected and frame_count % 60 == 0:\n",
    "                    self.metrics['collisions'] += 1\n",
    "                \n",
    "                # Capture screenshots\n",
    "                if current_time - last_screenshot >= screenshot_interval:\n",
    "                    sim.current_env.draw(sim.screen)\n",
    "                    sim.draw_hud()\n",
    "                    pygame.display.flip()\n",
    "                    \n",
    "                    screenshot = self._capture_screen(sim.screen)\n",
    "                    self.screenshots.append({\n",
    "                        'time': current_time,\n",
    "                        'image': screenshot,\n",
    "                        'environment': env_type,\n",
    "                        'collision': sim.current_env.collision_detected\n",
    "                    })\n",
    "                    \n",
    "                    last_screenshot = current_time\n",
    "                    print(f\"üì∏ t={current_time:.1f}s - Captured screenshot\")\n",
    "                \n",
    "                frame_count += 1\n",
    "                time.sleep(1/60)  # 60 FPS\n",
    "        \n",
    "        except KeyboardInterrupt:\n",
    "            print(\"‚èπÔ∏è  Simulation stopped by user\")\n",
    "        \n",
    "        # Calculate final metrics\n",
    "        self.metrics['simulation_time'] = time.time() - start_time\n",
    "        if speed_samples:\n",
    "            self.metrics['avg_speed'] = np.mean(speed_samples)\n",
    "            self.metrics['total_distance'] = np.sum(speed_samples) * (1/60)  # Approximate\n",
    "        \n",
    "        # Save simulation data\n",
    "        run_id = None\n",
    "        if sim.data_collection_enabled and sim.current_run_id:\n",
    "            sim.data_repository.end_current_run()\n",
    "            run_id = sim.current_run_id\n",
    "            print(f\"üíæ Simulation data saved: {run_id}\")\n",
    "        \n",
    "        pygame.quit()\n",
    "        \n",
    "        return {\n",
    "            'run_id': run_id,\n",
    "            'environment': env_type,\n",
    "            'duration': duration,\n",
    "            'metrics': self.metrics.copy(),\n",
    "            'repository': sim.data_repository if sim.data_collection_enabled else None\n",
    "        }\n",
    "    \n",
    "    def _capture_screen(self, screen):\n",
    "        \"\"\"Capture pygame screen as PIL image\"\"\"\n",
    "        w, h = screen.get_size()\n",
    "        raw = pygame.image.tostring(screen, 'RGB')\n",
    "        return Image.frombytes('RGB', (w, h), raw)\n",
    "    \n",
    "    def run_batch_simulations(self, configs):\n",
    "        \"\"\"Run multiple simulations with different configurations\"\"\"\n",
    "        results = []\n",
    "        \n",
    "        for i, config in enumerate(configs):\n",
    "            print(f\"\\nüîÑ Running simulation {i+1}/{len(configs)}\")\n",
    "            print(f\"‚öôÔ∏è  Config: {config}\")\n",
    "            \n",
    "            # Reset metrics for each run\n",
    "            self.metrics = {\n",
    "                'total_distance': 0,\n",
    "                'max_speed': 0,\n",
    "                'avg_speed': 0,\n",
    "                'collisions': 0,\n",
    "                'lane_changes': 0,\n",
    "                'simulation_time': 0\n",
    "            }\n",
    "            \n",
    "            result = self.run_environment(\n",
    "                env_type=config.get('environment', 'highway'),\n",
    "                duration=config.get('duration', 30),\n",
    "                auto_pilot=config.get('auto_pilot', True)\n",
    "            )\n",
    "            \n",
    "            result['config'] = config\n",
    "            results.append(result)\n",
    "            \n",
    "            print(f\"‚úÖ Completed: {result['metrics']}\")\n",
    "        \n",
    "        return results\n",
    "    \n",
    "    def show_screenshots(self, max_images=9):\n",
    "        \"\"\"Display captured screenshots in a grid\"\"\"\n",
    "        if not self.screenshots:\n",
    "            print(\"No screenshots available\")\n",
    "            return\n",
    "        \n",
    "        n_images = min(len(self.screenshots), max_images)\n",
    "        cols = 3\n",
    "        rows = (n_images + cols - 1) // cols\n",
    "        \n",
    "        fig, axes = plt.subplots(rows, cols, figsize=(15, 5*rows))\n",
    "        if rows == 1:\n",
    "            axes = [axes] if cols == 1 else axes\n",
    "        else:\n",
    "            axes = axes.flatten()\n",
    "        \n",
    "        for i in range(n_images):\n",
    "            screenshot = self.screenshots[i]\n",
    "            axes[i].imshow(screenshot['image'])\n",
    "            \n",
    "            title = f\"t={screenshot['time']:.1f}s - {screenshot['environment']}\"\n",
    "            if screenshot['collision']:\n",
    "                title += \" (COLLISION!)\"\n",
    "                axes[i].set_facecolor('red')\n",
    "            \n",
    "            axes[i].set_title(title)\n",
    "            axes[i].axis('off')\n",
    "        \n",
    "        # Hide empty subplots\n",
    "        for i in range(n_images, len(axes)):\n",
    "            axes[i].axis('off')\n",
    "        \n",
    "        plt.tight_layout()\n",
    "        plt.show()\n",
    "\n",
    "# Create runner instance\n",
    "runner = AdvancedSimulationRunner()\n",
    "print(\"‚úÖ Advanced simulation runner created!\")"
   ]
  },
