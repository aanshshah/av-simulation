    "def create_animated_playback(viz_data, max_frames=100):\n",
    "    \"\"\"Create animated playback of simulation\"\"\"\n",
    "    \n",
    "    if viz_data['vehicles'].empty:\n",
    "        print(\"No data available for animation\")\n",
    "        return\n",
    "    \n",
    "    df = viz_data['vehicles_enhanced']\n",
    "    ego_df = df[df['is_ego'] == True]\n",
    "    \n",
    "    if ego_df.empty:\n",
    "        print(\"No ego vehicle data for animation\")\n",
    "        return\n",
    "    \n",
    "    print(\"🎬 Creating Animated Simulation Playback...\")\n",
    "    \n",
    "    # Sample data to reasonable number of frames\n",
    "    if len(ego_df) > max_frames:\n",
    "        step = len(ego_df) // max_frames\n",
    "        ego_sample = ego_df.iloc[::step].copy()\n",
    "    else:\n",
    "        ego_sample = ego_df.copy()\n",
    "    \n",
    "    # Create animated trajectory\n",
    "    fig = go.Figure()\n",
    "    \n",
    "    # Add trajectory trail\n",
    "    fig.add_trace(go.Scatter(\n",
    "        x=ego_sample['position_x'],\n",
    "        y=ego_sample['position_y'],\n",
    "        mode='lines',\n",
    "        line=dict(color='lightgray', width=2),\n",
    "        name='Trajectory Trail',\n",
    "        opacity=0.5\n",
    "    ))\n",
    "    \n",
    "    # Animation frames\n",
    "    frames = []\n",
    "    \n",
    "    for i in range(len(ego_sample)):\n",
    "        current_data = ego_sample.iloc[:i+1]\n",
    "        current_point = ego_sample.iloc[i]\n",
    "        \n",
    "        # Create frame\n",
    "        frame = go.Frame(\n",
    "            data=[\n",
    "                # Trail\n",
    "                go.Scatter(\n",
    "                    x=current_data['position_x'],\n",
    "                    y=current_data['position_y'],\n",
    "                    mode='lines',\n",
    "                    line=dict(color='blue', width=3),\n",
    "                    name='Path'\n",
    "                ),\n",
    "                # Current position\n",
    "                go.Scatter(\n",
    "                    x=[current_point['position_x']],\n",
    "                    y=[current_point['position_y']],\n",
    "                    mode='markers',\n",
    "                    marker=dict(\n",
    "                        size=15,\n",
    "                        color=current_point['speed'],\n",
    "                        colorscale='Viridis',\n",
    "                        cmin=ego_sample['speed'].min(),\n",
    "                        cmax=ego_sample['speed'].max(),\n",
    "                        line=dict(width=2, color='black')\n",
    "                    ),\n",
    "                    name='Vehicle'\n",
    "                )\n",
    "            ],\n",
    "            name=f\"t={current_point['timestamp']:.1f}s\"\n",
    "        )\n",
    "        frames.append(frame)\n",
    "    \n",
    "    fig.frames = frames\n",
    "    \n",
    "    # Initial frame\n",
    "    fig.add_trace(go.Scatter(\n",
    "        x=[ego_sample.iloc[0]['position_x']],\n",
    "        y=[ego_sample.iloc[0]['position_y']],\n",
    "        mode='markers',\n",
    "        marker=dict(\n",
    "            size=15,\n",
    "            color=ego_sample.iloc[0]['speed'],\n",
    "            colorscale='Viridis',\n",
    "            cmin=ego_sample['speed'].min(),\n",
    "            cmax=ego_sample['speed'].max(),\n",
    "            line=dict(width=2, color='black'),\n",
    "            colorbar=dict(title=\"Speed (m/s)\")\n",
    "        ),\n",
    "        name='Vehicle'\n",
    "    ))\n",
    "    \n",
    "    # Animation controls\n",
    "    fig.update_layout(\n",
    "        title=\"🎬 Animated Vehicle Simulation Playback\",\n",
    "        xaxis_title=\"Position X (m)\",\n",
    "        yaxis_title=\"Position Y (m)\",\n",
    "        template='plotly_white',\n",
    "        updatemenus=[{\n",
    "            \"buttons\": [\n",
    "                {\n",
    "                    \"args\": [None, {\"frame\": {\"duration\": 100, \"redraw\": True},\n",
    "                                    \"fromcurrent\": True, \"transition\": {\"duration\": 50}}],\n",
    "                    \"label\": \"▶️ Play\",\n",
    "                    \"method\": \"animate\"\n",
    "                },\n",
    "                {\n",
    "                    \"args\": [[None], {\"frame\": {\"duration\": 0, \"redraw\": True},\n",
    "                                     \"mode\": \"immediate\", \"transition\": {\"duration\": 0}}],\n",
    "                    \"label\": \"⏸️ Pause\",\n",
    "                    \"method\": \"animate\"\n",
    "                }\n",
    "            ],\n",
    "            \"direction\": \"left\",\n",
    "            \"pad\": {\"r\": 10, \"t\": 87},\n",
    "            \"showactive\": False,\n",
    "            \"type\": \"buttons\",\n",
    "            \"x\": 0.1,\n",
    "            \"xanchor\": \"right\",\n",
    "            \"y\": 0,\n",
    "            \"yanchor\": \"top\"\n",
    "        }],\n",
    "        sliders=[{\n",
    "            \"active\": 0,\n",
    "            \"yanchor\": \"top\",\n",
    "            \"xanchor\": \"left\",\n",
    "            \"currentvalue\": {\n",
    "                \"font\": {\"size\": 20},\n",
    "                \"prefix\": \"Time: \",\n",
    "                \"visible\": True,\n",
    "                \"xanchor\": \"right\"\n",
    "            },\n",
    "            \"transition\": {\"duration\": 300, \"easing\": \"cubic-in-out\"},\n",
    "            \"len\": 0.9,\n",
    "            \"x\": 0.1,\n",
    "            \"y\": 0,\n",
    "            \"steps\": [\n",
    "                {\n",
    "                    \"args\": [[f.name], {\n",
    "                        \"frame\": {\"duration\": 300, \"redraw\": True},\n",
    "                        \"mode\": \"immediate\",\n",
    "                        \"transition\": {\"duration\": 300}\n",
    "                    }],\n",
    "                    \"label\": f.name,\n",
    "                    \"method\": \"animate\"\n",
    "                }\n",
    "                for f in frames\n",
    "            ]\n",
    "        }],\n",
    "        height=600\n",
    "    )\n",
    "    \n",
    "    fig.show()\n",
    "    \n",
    "    # Create speed gauge animation\n",
    "    print(\"📊 Creating Speed Gauge Animation...\")\n",
    "    \n",
    "    gauge_fig = go.Figure()\n",
    "    \n",
    "    gauge_frames = []\n",
    "    for i, (_, row) in enumerate(ego_sample.iterrows()):\n",
    "        frame = go.Frame(\n",
    "            data=[go.Indicator(\n",
    "                mode=\"gauge+number+delta\",\n",
    "                value=row['speed'],\n",
    "                delta={'reference': 25, 'increasing': {'color': \"red\"}, 'decreasing': {'color': \"green\"}},\n",
    "                gauge={\n",
    "                    'axis': {'range': [None, 50]},\n",
    "                    'bar': {'color': \"darkblue\"},\n",
    "                    'steps': [\n",
    "                        {'range': [0, 20], 'color': \"lightgray\"},\n",
    "                        {'range': [20, 30], 'color': \"yellow\"},\n",
    "                        {'range': [30, 50], 'color': \"red\"}\n",
    "                    ],\n",
    "                    'threshold': {\n",
    "                        'line': {'color': \"red\", 'width': 4},\n",
    "                        'thickness': 0.75,\n",
    "                        'value': 30\n",
    "                    }\n",
    "                },\n",
    "                title={'text': f\"Speed at t={row['timestamp']:.1f}s\"},\n",
    "                subtitle={'text': \"m/s\"}\n",
    "            )],\n",
    "            name=f\"t={row['timestamp']:.1f}s\"\n",
    "        )\n",
    "        gauge_frames.append(frame)\n",
    "    \n",
    "    gauge_fig.frames = gauge_frames\n",
    "    \n",
    "    # Initial gauge\n",
    "    gauge_fig.add_trace(go.Indicator(\n",
    "        mode=\"gauge+number+delta\",\n",
    "        value=ego_sample.iloc[0]['speed'],\n",
    "        delta={'reference': 25},\n",
    "        gauge={\n",
    "            'axis': {'range': [None, 50]},\n",
    "            'bar': {'color': \"darkblue\"},\n",
    "            'steps': [\n",
    "                {'range': [0, 20], 'color': \"lightgray\"},\n",
    "                {'range': [20, 30], 'color': \"yellow\"},\n",
    "                {'range': [30, 50], 'color': \"red\"}\n",
    "            ],\n",
    "            'threshold': {\n",
    "                'line': {'color': \"red\", 'width': 4},\n",
    "                'thickness': 0.75,\n",
    "                'value': 30\n",
    "            }\n",
    "        },\n",
    "        title={'text': \"Vehicle Speed Monitor\"},\n",
    "        subtitle={'text': \"m/s\"}\n",
    "    ))\n",
    "    \n",
    "    gauge_fig.update_layout(\n",
    "        title=\"📊 Real-time Speed Monitor\",\n",
    "        template='plotly_white',\n",
    "        height=500,\n",
    "        updatemenus=[{\n",
    "            \"buttons\": [\n",
    "                {\n",
    "                    \"args\": [None, {\"frame\": {\"duration\": 100, \"redraw\": True},\n",
    "                                    \"fromcurrent\": True}],\n",
    "                    \"label\": \"▶️ Play\",\n",
    "                    \"method\": \"animate\"\n",
    "                },\n",
    "                {\n",
    "                    \"args\": [[None], {\"frame\": {\"duration\": 0, \"redraw\": True},\n",
    "                                     \"mode\": \"immediate\"}],\n",
    "                    \"label\": \"⏸️ Pause\",\n",
    "                    \"method\": \"animate\"\n",
    "                }\n",
    "            ],\n",
    "            \"direction\": \"left\",\n",
    "            \"pad\": {\"r\": 10, \"t\": 87},\n",
    "            \"showactive\": False,\n",
    "            \"type\": \"buttons\",\n",
    "            \"x\": 0.1,\n",
    "            \"y\": 0,\n",
    "        }]\n",
    "    )\n",
    "    \n",
    "    gauge_fig.show()\n",
    "    \n",
    "    print(\"✅ Animated visualizations created!\")\n",
    "    print(\"🎬 Use play/pause controls to control playback\")\n",
