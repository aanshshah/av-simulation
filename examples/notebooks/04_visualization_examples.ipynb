    "def create_interactive_dashboard(viz_data):\n",
    "    \"\"\"Create interactive Plotly dashboard\"\"\"\n",
    "    \n",
    "    if viz_data['vehicles'].empty:\n",
    "        print(\"No data available for interactive plots\")\n",
    "        return\n",
    "    \n",
    "    df = viz_data['vehicles_enhanced']\n",
    "    ego_df = df[df['is_ego'] == True]\n",
    "    \n",
    "    print(\"🎮 Creating Interactive Dashboard...\")\n",
    "    \n",
    "    # 1. Interactive Trajectory Plot\n",
    "    fig1 = go.Figure()\n",
    "    \n",
    "    if not ego_df.empty:\n",
    "        # Add ego vehicle trajectory\n",
    "        fig1.add_trace(go.Scatter(\n",
    "            x=ego_df['position_x'],\n",
    "            y=ego_df['position_y'],\n",
    "            mode='markers',\n",
    "            marker=dict(\n",
    "                size=8,\n",
    "                color=ego_df['speed'],\n",
    "                colorscale='Viridis',\n",
    "                colorbar=dict(title=\"Speed (m/s)\"),\n",
    "                line=dict(width=1, color='black')\n",
    "            ),\n",
    "            text=[f\"Time: {t:.1f}s<br>Speed: {s:.1f} m/s<br>Accel: {a:.2f} m/s²\" \n",
    "                  for t, s, a in zip(ego_df['timestamp'], ego_df['speed'], ego_df['acceleration'])],\n",
    "            hovertemplate=\"<b>Position:</b> (%{x:.1f}, %{y:.1f})<br>%{text}<extra></extra>\",\n",
    "            name=\"Ego Vehicle Trajectory\"\n",
    "        ))\n",
    "    \n",
    "    # Add other vehicles if available\n",
    "    other_df = df[df['is_ego'] == False]\n",
    "    if not other_df.empty:\n",
    "        # Sample to avoid overcrowding\n",
    "        other_sample = other_df.sample(min(200, len(other_df))) if len(other_df) > 200 else other_df\n",
    "        \n",
    "        fig1.add_trace(go.Scatter(\n",
    "            x=other_sample['position_x'],\n",
    "            y=other_sample['position_y'],\n",
    "            mode='markers',\n",
    "            marker=dict(\n",
    "                size=5,\n",
    "                color='lightblue',\n",
    "                opacity=0.6,\n",
    "                line=dict(width=1, color='darkblue')\n",
    "            ),\n",
    "            text=[f\"Other Vehicle<br>Speed: {s:.1f} m/s\" for s in other_sample['speed']],\n",
