    "# Create utility functions for custom visualizations\n",
    "def create_safety_dashboard(viz_data):\n",
    "    \"\"\"Create comprehensive safety dashboard\"\"\"\n",
    "    \n",
    "    if viz_data['vehicles'].empty:\n",
    "        print(\"No data available for safety dashboard\")\n",
    "        return\n",
    "    \n",
    "    df = viz_data['vehicles_enhanced']\n",
    "    ego_df = df[df['is_ego'] == True]\n",
    "    collision_df = viz_data['collisions']\n",
    "    \n",
    "    print(\"⚠️ Creating Safety Analysis Dashboard...\")\n",
    "    \n",
    "    # Create subplot figure\n",
    "    fig = make_subplots(\n",
    "        rows=2, cols=3,\n",
    "        subplot_titles=(\n",
    "            'Safety Score Timeline', 'Risk Distribution', 'Speed Violations',\n",
    "            'Collision Analysis', 'Hard Events', 'Safety Metrics Summary'\n",
    "        ),\n",
    "        specs=[[{\"secondary_y\": True}, {\"type\": \"pie\"}, {}],\n",
    "               [{\"type\": \"scatter\"}, {\"type\": \"bar\"}, {\"type\": \"indicator\"}]]\n",
    "    )\n",
    "    \n",
    "    if not ego_df.empty:\n",
    "        # 1. Safety Score Timeline\n",
    "        safety_score = 100 - (\n",
    "            (ego_df['speed'] / 40 * 30) +\n",
    "            (abs(ego_df['acceleration']) / 5 * 25) +\n",
    "            (abs(ego_df['steering_angle']) / 0.785 * 20)\n",
    "        ).clip(0, 100)\n",
    "        \n",
    "        fig.add_trace(\n",
    "            go.Scatter(x=ego_df['timestamp'], y=safety_score, name='Safety Score',\n",
    "                      line=dict(color='green', width=3)),\n",
    "            row=1, col=1\n",
    "        )\n",
    "        \n",
    "        # Add threshold lines\n",
    "        fig.add_hline(y=70, line_dash=\"dash\", line_color=\"orange\", row=1, col=1)\n",
    "        fig.add_hline(y=50, line_dash=\"dash\", line_color=\"red\", row=1, col=1)\n",
    "        \n",
    "        # 2. Risk Distribution\n",
    "        risk_counts = ego_df['risk_level'].value_counts()\n",
    "        colors = [AV_COLORS['safe'], AV_COLORS['warning'], AV_COLORS['danger']]\n",
    "        \n",
    "        fig.add_trace(\n",
    "            go.Pie(labels=risk_counts.index, values=risk_counts.values,\n",
    "                  marker_colors=colors[:len(risk_counts)]),\n",
    "            row=1, col=2\n",
    "        )\n",
    "        \n",
    "        # 3. Speed Violations\n",
    "        speed_violations = ego_df[ego_df['speed'] > 30]\n",
    "        \n",
    "        if not speed_violations.empty:\n",
    "            fig.add_trace(\n",
    "                go.Scatter(x=speed_violations['timestamp'], y=speed_violations['speed'],\n",
    "                          mode='markers', marker=dict(color='red', size=8),\n",
    "                          name='Speed Violations'),\n",
    "                row=1, col=3\n",
    "            )\n",
    "        \n",
    "        fig.add_hline(y=30, line_dash=\"dash\", line_color=\"red\", row=1, col=3)\n",
    "        \n",
    "        # 4. Collision Analysis\n",
    "        if not collision_df.empty:\n",
    "            fig.add_trace(\n",
    "                go.Scatter(x=collision_df['collision_point_x'], \n",
    "                          y=collision_df['collision_point_y'],\n",
    "                          mode='markers',\n",
    "                          marker=dict(size=collision_df['relative_speed']*2,\n",
    "                                     color='red', opacity=0.7),\n",
    "                          name='Collisions'),\n",
    "                row=2, col=1\n",
    "            )\n",
    "        else:\n",
    "            fig.add_annotation(\n",
    "                text=\"✅ No Collisions\", x=0.5, y=0.5, xref=\"x4\", yref=\"y4\",\n",
    "                showarrow=False, font=dict(size=16, color=\"green\")\n",
    "            )\n",
    "        \n",
    "        # 5. Hard Events\n",
    "        hard_accel = len(ego_df[ego_df['acceleration'] > 3])\n",
    "        hard_brake = len(ego_df[ego_df['acceleration'] < -3])\n",
    "        \n",
    "        fig.add_trace(\n",
    "            go.Bar(x=['Hard Acceleration', 'Hard Braking'], \n",
    "                  y=[hard_accel, hard_brake],\n",
    "                  marker_color=['orange', 'red']),\n",
    "            row=2, col=2\n",
    "        )\n",
    "        \n",
    "        # 6. Safety Metrics Summary (Gauge)\n",
    "        overall_safety = safety_score.mean()\n",
    "        \n",
    "        fig.add_trace(\n",
    "            go.Indicator(\n",
    "                mode=\"gauge+number\",\n",
    "                value=overall_safety,\n",
    "                gauge=dict(\n",
    "                    axis=dict(range=[0, 100]),\n",
    "                    bar=dict(color=\"darkgreen\"),\n",
    "                    steps=[\n",
    "                        dict(range=[0, 50], color=\"red\"),\n",
    "                        dict(range=[50, 70], color=\"yellow\"),\n",
    "                        dict(range=[70, 100], color=\"green\")\n",
    "                    ]\n",
    "                ),\n",
    "                title=dict(text=\"Overall Safety Score\")\n",
    "            ),\n",
    "            row=2, col=3\n",
    "        )\n",
    "    \n",
    "    fig.update_layout(\n",
    "        title=\"⚠️ Comprehensive Safety Analysis Dashboard\",\n",
    "        template='plotly_white',\n",
    "        height=800,\n",
    "        showlegend=False\n",
    "    )\n",
    "    \n",
    "    fig.show()\n",
    "    \n",
    "    return fig\n",
    "\n",
    "def create_performance_comparison(viz_data):\n",
    "    \"\"\"Create performance comparison visualization\"\"\"\n",
    "    \n",
    "    if viz_data['vehicles'].empty:\n",
    "        print(\"No data available for performance comparison\")\n",
    "        return\n",
    "    \n",
    "    df = viz_data['vehicles_enhanced']\n",
    "    ego_df = df[df['is_ego'] == True]\n",
    "    \n",
    "    print(\"🏎️ Creating Performance Comparison...\")\n",
    "    \n",
    "    if not ego_df.empty:\n",
    "        # Performance metrics\n",
    "        metrics = {\n",
    "            'Average Speed': ego_df['speed'].mean(),\n",
    "            'Speed Consistency': 100 - ego_df['speed'].std(),\n",
    "            'Smooth Driving': 100 - abs(ego_df['acceleration']).mean() * 10,\n",
    "            'Energy Efficiency': 100 - (abs(ego_df['acceleration'])**2).mean() * 5\n",
    "        }\n",
    "        \n",
    "        # Normalize to 0-100 scale\n",
    "        normalized_metrics = {k: max(0, min(100, v)) for k, v in metrics.items()}\n",
    "        \n",
    "        # Create radar chart\n",
    "        fig = go.Figure()\n",
    "        \n",
    "        fig.add_trace(go.Scatterpolar(\n",
    "            r=list(normalized_metrics.values()),\n",
    "            theta=list(normalized_metrics.keys()),\n",
    "            fill='toself',\n",
    "            name='Current Performance',\n",
    "            line_color='blue'\n",
    "        ))\n",
    "        \n",
    "        # Add ideal performance reference\n",
    "        ideal_performance = [85, 90, 85, 80]  # Target values\n",
    "        \n",
    "        fig.add_trace(go.Scatterpolar(\n",
    "            r=ideal_performance,\n",
    "            theta=list(normalized_metrics.keys()),\n",
    "            fill='toself',\n",
    "            name='Target Performance',\n",
    "            line_color='green',\n",
    "            opacity=0.5\n",
    "        ))\n",
    "        \n",
    "        fig.update_layout(\n",
    "            polar=dict(\n",
    "                radialaxis=dict(\n",
    "                    visible=True,\n",
    "                    range=[0, 100]\n",
    "                )\n",
    "            ),\n",
    "            title=\"🏎️ Performance Comparison Radar Chart\",\n",
    "            template='plotly_white',\n",
    "            height=600\n",
    "        )\n",
    "        \n",
    "        fig.show()\n",
    "        \n",
    "        return fig\n",
    "\n",
    "# Create custom dashboards\n",
    "safety_dashboard = create_safety_dashboard(viz_data)\n",
    "performance_radar = create_performance_comparison(viz_data)\n",
    "\n",
    "print(\"\\n✅ Custom visualization utilities created!\")\n",
    "print(\"⚠️  Safety dashboard provides comprehensive risk analysis\")\n",
    "print(\"🏎️ Performance radar shows multi-dimensional comparison\")"
