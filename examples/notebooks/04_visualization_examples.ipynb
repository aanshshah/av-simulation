    "def create_publication_plots(viz_data):\n",
    "    \"\"\"Create high-quality publication-ready plots\"\"\"\n",
    "    \n",
    "    if viz_data['vehicles'].empty:\n",
    "        print(\"No data available for publication plots\")\n",
    "        return\n",
    "    \n",
    "    df = viz_data['vehicles_enhanced']\n",
    "    ego_df = df[df['is_ego'] == True]\n",
    "    \n",
    "    # Set up the figure with custom styling\n",
    "    plt.rcParams.update({\n",
    "        'font.size': 12,\n",
    "        'font.family': 'serif',\n",
    "        'axes.linewidth': 1.5,\n",
    "        'axes.spines.top': False,\n",
    "        'axes.spines.right': False,\n",
    "        'xtick.direction': 'in',\n",
    "        'ytick.direction': 'in',\n",
    "        'xtick.major.size': 6,\n",
    "        'ytick.major.size': 6,\n",
    "        'legend.frameon': True,\n",
    "        'legend.fancybox': True,\n",
    "        'legend.shadow': True\n",
    "    })\n",
    "    \n",
    "    # Create publication figure\n",
    "    fig = plt.figure(figsize=(16, 12))\n",
    "    gs = fig.add_gridspec(3, 3, hspace=0.3, wspace=0.3)\n",
    "    \n",
    "    # 1. Vehicle Trajectory with Risk Coloring\n",
    "    ax1 = fig.add_subplot(gs[0, :])\n",
    "    \n",
    "    if not ego_df.empty:\n",
    "        # Create risk-based colormap\n",
    "        risk_colors = {'Low': AV_COLORS['safe'], \n",
    "                      'Medium': AV_COLORS['warning'], \n",
    "                      'High': AV_COLORS['danger']}\n",
    "        \n",
    "        for risk_level in ['Low', 'Medium', 'High']:\n",
    "            risk_data = ego_df[ego_df['risk_level'] == risk_level]\n",
    "            if not risk_data.empty:\n",
    "                ax1.scatter(risk_data['position_x'], risk_data['position_y'], \n",
    "                           c=risk_colors[risk_level], s=15, alpha=0.7, \n",
    "                           label=f'{risk_level} Risk', edgecolors='black', linewidth=0.5)\n",
    "        \n",
    "        ax1.set_xlabel('Position X (m)', fontweight='bold')\n",
    "        ax1.set_ylabel('Position Y (m)', fontweight='bold')\n",
    "        ax1.set_title('Vehicle Trajectory Analysis\\nColored by Risk Level', \n",
    "                     fontsize=14, fontweight='bold', pad=20)\n",
    "        ax1.legend(loc='upper right', framealpha=0.9)\n",
    "        ax1.grid(True, alpha=0.3, linestyle='--')\n",
    "        ax1.set_aspect('equal')\n",
    "    \n",
    "    # 2. Speed Distribution with Statistics\n",
    "    ax2 = fig.add_subplot(gs[1, 0])\n",
    "    \n",
    "    ego_speeds = ego_df['speed'] if not ego_df.empty else []\n",
    "    other_speeds = df[df['is_ego'] == False]['speed'] if not df[df['is_ego'] == False].empty else []\n",
    "    \n",
    "    if len(ego_speeds) > 0:\n",
    "        ax2.hist(ego_speeds, bins=25, alpha=0.7, color=AV_COLORS['ego'], \n",
    "                label='Ego Vehicle', density=True, edgecolor='black', linewidth=1)\n",
    "    \n",
    "    if len(other_speeds) > 0:\n",
    "        ax2.hist(other_speeds, bins=25, alpha=0.5, color=AV_COLORS['other'], \n",
    "                label='Other Vehicles', density=True, edgecolor='black', linewidth=1)\n",
    "    \n",
    "    # Add statistical annotations\n",
    "    if len(ego_speeds) > 0:\n",
    "        mean_speed = np.mean(ego_speeds)\n",
    "        std_speed = np.std(ego_speeds)\n",
    "        ax2.axvline(mean_speed, color=AV_COLORS['ego'], linestyle='--', linewidth=2, alpha=0.8)\n",
    "        ax2.text(mean_speed + 1, ax2.get_ylim()[1] * 0.8, \n",
    "                f'μ = {mean_speed:.1f} m/s\\nσ = {std_speed:.1f}', \n",
    "                bbox=dict(boxstyle='round', facecolor='white', alpha=0.8))\n",
    "    \n",
    "    ax2.set_xlabel('Speed (m/s)', fontweight='bold')\n",
    "    ax2.set_ylabel('Density', fontweight='bold')\n",
    "    ax2.set_title('Speed Distribution Analysis', fontweight='bold')\n",
    "    ax2.legend(framealpha=0.9)\n",
    "    ax2.grid(True, alpha=0.3, linestyle='--')\n",
    "    \n",
    "    # 3. Acceleration vs Speed Scatter with Density\n",
    "    ax3 = fig.add_subplot(gs[1, 1])\n",
    "    \n",
    "    if not ego_df.empty:\n",
    "        # Create hexbin plot for density\n",
    "        hb = ax3.hexbin(ego_df['speed'], ego_df['acceleration'], \n",
    "                       gridsize=20, cmap='Blues', alpha=0.8, mincnt=1)\n",
    "        \n",
    "        # Add zero acceleration line\n",
    "        ax3.axhline(y=0, color='red', linestyle='--', alpha=0.7, linewidth=2)\n",
    "        \n",
    "        # Safety boundaries\n",
    "        ax3.axhline(y=3, color=AV_COLORS['warning'], linestyle=':', alpha=0.7, \n",
    "                   label='Hard Acceleration')\n",
    "        ax3.axhline(y=-3, color=AV_COLORS['danger'], linestyle=':', alpha=0.7, \n",
    "                   label='Hard Braking')\n",
    "        \n",
    "        cb = plt.colorbar(hb, ax=ax3)\n",
    "        cb.set_label('Data Point Density', fontweight='bold')\n",
    "    \n",
    "    ax3.set_xlabel('Speed (m/s)', fontweight='bold')\n",
    "    ax3.set_ylabel('Acceleration (m/s²)', fontweight='bold')\n",
    "    ax3.set_title('Speed vs Acceleration\\nDensity Plot', fontweight='bold')\n",
    "    ax3.legend(framealpha=0.9)\n",
    "    ax3.grid(True, alpha=0.3, linestyle='--')\n",
    "    \n",
    "    # 4. Time Series Analysis\n",
    "    ax4 = fig.add_subplot(gs[1, 2])\n",
    "    \n",
    "    if not ego_df.empty:\n",
    "        # Rolling statistics\n",
    "        window = min(50, len(ego_df) // 10)\n",
    "        if window > 1:\n",
    "            rolling_mean = ego_df['speed'].rolling(window=window).mean()\n",
    "            rolling_std = ego_df['speed'].rolling(window=window).std()\n",
    "            \n",
    "            ax4.plot(ego_df['timestamp'], ego_df['speed'], alpha=0.3, color='lightblue', linewidth=1)\n",
    "            ax4.plot(ego_df['timestamp'], rolling_mean, color=AV_COLORS['ego'], linewidth=2, label='Rolling Mean')\n",
    "            ax4.fill_between(ego_df['timestamp'], \n",
    "                           rolling_mean - rolling_std, \n",
    "                           rolling_mean + rolling_std, \n",
    "                           alpha=0.2, color=AV_COLORS['ego'], label='±1σ')\n",
    "    \n",
    "    ax4.set_xlabel('Time (s)', fontweight='bold')\n",
    "    ax4.set_ylabel('Speed (m/s)', fontweight='bold')\n",
    "    ax4.set_title('Speed Time Series\\nwith Rolling Statistics', fontweight='bold')\n",
    "    ax4.legend(framealpha=0.9)\n",
    "    ax4.grid(True, alpha=0.3, linestyle='--')\n",
    "    \n",
    "    # 5. Performance Metrics Dashboard\n",
    "    ax5 = fig.add_subplot(gs[2, 0])\n",
    "    \n",
    "    if not ego_df.empty:\n",
    "        # Calculate key metrics\n",
    "        metrics = {\n",
    "            'Avg Speed\\n(m/s)': ego_df['speed'].mean(),\n",
    "            'Max Speed\\n(m/s)': ego_df['speed'].max(),\n",
    "            'Speed Variance\\n(m²/s²)': ego_df['speed'].var(),\n",
    "            'Hard Events\\n(count)': len(ego_df[abs(ego_df['acceleration']) > 3])\n",
    "        }\n",
    "        \n",
    "        bars = ax5.bar(range(len(metrics)), list(metrics.values()), \n",
    "                      color=[AV_COLORS['ego'], AV_COLORS['warning'], \n",
    "                            AV_COLORS['neutral'], AV_COLORS['danger']], \n",
    "                      alpha=0.8, edgecolor='black', linewidth=1)\n",
    "        \n",
    "        # Add value labels on bars\n",
    "        for bar, value in zip(bars, metrics.values()):\n",
    "            height = bar.get_height()\n",
    "            ax5.text(bar.get_x() + bar.get_width()/2., height + max(metrics.values())*0.01,\n",
    "                    f'{value:.1f}', ha='center', va='bottom', fontweight='bold')\n",
    "        \n",
    "        ax5.set_xticks(range(len(metrics)))\n",
    "        ax5.set_xticklabels(list(metrics.keys()), rotation=45, ha='right')\n",
    "        ax5.set_ylabel('Value', fontweight='bold')\n",
    "        ax5.set_title('Key Performance Metrics', fontweight='bold')\n",
    "        ax5.grid(True, alpha=0.3, linestyle='--', axis='y')\n",
    "    \n",
    "    # 6. Safety Analysis\n",
    "    ax6 = fig.add_subplot(gs[2, 1])\n",
    "    \n",
    "    if not viz_data['collisions'].empty:\n",
    "        collision_df = viz_data['collisions']\n",
    "        \n",
    "        # Collision severity analysis\n",
    "        severity_bins = [0, 5, 15, 30, 100]\n",
    "        severity_labels = ['Minor', 'Moderate', 'Severe', 'Critical']\n",
    "        collision_df['severity'] = pd.cut(collision_df['relative_speed'], \n",
    "                                        bins=severity_bins, labels=severity_labels)\n",
    "        \n",
    "        severity_counts = collision_df['severity'].value_counts()\n",
    "        colors = [AV_COLORS['safe'], AV_COLORS['warning'], AV_COLORS['danger'], AV_COLORS['collision']]\n",
    "        \n",
    "        wedges, texts, autotexts = ax6.pie(severity_counts.values, \n",
    "                                          labels=severity_counts.index,\n",
    "                                          autopct='%1.1f%%', \n",
    "                                          colors=colors[:len(severity_counts)],\n",
    "                                          startangle=90, \n",
    "                                          explode=[0.05] * len(severity_counts))\n",
    "        \n",
    "        ax6.set_title('Collision Severity\\nDistribution', fontweight='bold')\n",
    "    else:\n",
    "        # No collisions - show safety status\n",
    "        ax6.text(0.5, 0.5, '✅\\nNO COLLISIONS\\nDETECTED', \n",
    "                ha='center', va='center', transform=ax6.transAxes,\n",
    "                fontsize=16, fontweight='bold', color=AV_COLORS['safe'],\n",
    "                bbox=dict(boxstyle='round,pad=0.5', facecolor='lightgreen', alpha=0.3))\n",
    "        ax6.set_xlim(-1, 1)\n",
    "        ax6.set_ylim(-1, 1)\n",
    "        ax6.set_title('Safety Status', fontweight='bold')\n",
    "    \n",
    "    # 7. Action Analysis\n",
    "    ax7 = fig.add_subplot(gs[2, 2])\n",
    "    \n",
    "    if not viz_data['actions'].empty:\n",
    "        actions_df = viz_data['actions']\n",
    "        action_counts = actions_df['action'].value_counts()\n",
    "        \n",
    "        # Horizontal bar chart\n",
    "        bars = ax7.barh(range(len(action_counts)), action_counts.values, \n",
    "                       color=AV_COLORS['ego'], alpha=0.8, edgecolor='black', linewidth=1)\n",
    "        \n",
    "        ax7.set_yticks(range(len(action_counts)))\n",
    "        ax7.set_yticklabels(action_counts.index)\n",
    "        ax7.set_xlabel('Frequency', fontweight='bold')\n",
    "        ax7.set_title('Autonomous Actions\\nFrequency', fontweight='bold')\n",
    "        ax7.grid(True, alpha=0.3, linestyle='--', axis='x')\n",
    "        \n",
    "        # Add value labels\n",
    "        for i, (bar, value) in enumerate(zip(bars, action_counts.values)):\n",
    "            ax7.text(value + max(action_counts.values())*0.01, i, \n",
    "                    str(value), va='center', fontweight='bold')\n",
    "    else:\n",
    "        ax7.text(0.5, 0.5, 'No Action Data\\nAvailable', \n",
    "                ha='center', va='center', transform=ax7.transAxes,\n",
    "                fontsize=12, style='italic')\n",
    "    \n",
    "    # Add overall title and subtitle\n",
    "    fig.suptitle('Autonomous Vehicle Simulation Analysis', \n",
    "                fontsize=20, fontweight='bold', y=0.98)\n",
    "    \n",
    "    # Add metadata\n",
    "    if not df.empty:\n",
    "        metadata = f\"Dataset: {len(df):,} data points | Duration: {df['timestamp'].max():.1f}s | Generated: {pd.Timestamp.now().strftime('%Y-%m-%d %H:%M')}\"\n",
    "        fig.text(0.5, 0.02, metadata, ha='center', va='bottom', \n",
    "                fontsize=10, style='italic', alpha=0.7)\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    \n",
    "    # Save high-resolution version\n",
    "    plt.savefig('av_simulation_analysis.png', dpi=300, bbox_inches='tight', \n",
    "               facecolor='white', edgecolor='none')\n",
    "    plt.savefig('av_simulation_analysis.pdf', bbox_inches='tight', \n",
    "               facecolor='white', edgecolor='none')\n",
    "    \n",
    "    plt.show()\n",
    "    \n",
    "    print(\"📊 Publication plots created and saved:\")\n",
    "    print(\"  📸 av_simulation_analysis.png (300 DPI)\")\n",
    "    print(\"  📄 av_simulation_analysis.pdf (Vector)\")\n",
    "\n",
    "# Create publication plots\n",
    "create_publication_plots(viz_data)"
   ]
