   "source": [
    "# Standard imports\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "import plotly.express as px\n",
    "import plotly.graph_objects as go\n",
    "from plotly.subplots import make_subplots\n",
    "import plotly.figure_factory as ff\n",
    "from matplotlib.animation import FuncAnimation\n",
    "from IPython.display import HTML, display\n",
    "import warnings\n",
    "warnings.filterwarnings('ignore')\n",
    "\n",
    "# Styling\n",
    "plt.style.use('seaborn-v0_8-darkgrid')\n",
    "sns.set_palette(\"husl\")\n",
    "\n",
    "# Custom color palettes\n",
    "AV_COLORS = {\n",
    "    'ego': '#2E8B57',      # Sea Green\n",
    "    'other': '#4169E1',    # Royal Blue\n",
    "    'collision': '#DC143C', # Crimson\n",
    "    'safe': '#32CD32',     # Lime Green\n",
    "    'warning': '#FF8C00',  # Dark Orange\n",
    "    'danger': '#FF0000',   # Red\n",
    "    'neutral': '#708090'   # Slate Gray\n",
    "}\n",
    "\n",
    "# Check environment\n",
    "try:\n",
    "    from google.colab import files\n",
    "    IN_COLAB = True\n",
    "    data_path = \"/content/\"\nexcept ImportError:\n",
    "    IN_COLAB = False\n",
    "    data_path = \"../\"\n",
    "\n",
    "print(\"📊 Visualization Environment Ready\")\n",
    "print(f\"🎨 Custom AV color palette loaded\")\n",
    "print(f\"🖥️  Running in: {'Google Colab' if IN_COLAB else 'Local Environment'}\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "id": "load_viz_data"
   },
   "outputs": [],
   "source": [
    "# Load simulation data for visualization\n",
    "def load_visualization_data():\n",
    "    \"\"\"Load and prepare data for visualization\"\"\"\n",
    "    \n",
    "    data_files = {\n",
    "        'vehicles': 'exported_csv_data/all_vehicle_data.csv',\n",
    "        'environment': 'exported_csv_data/all_environment_data.csv',\n",
    "        'collisions': 'exported_csv_data/all_collision_events.csv',\n",
    "        'actions': 'exported_csv_data/all_action_events.csv'\n",
    "    }\n",
    "    \n",
    "    viz_data = {}\n",
    "    \n",
    "    for key, filename in data_files.items():\n",
    "        filepath = f\"{data_path}{filename}\"\n",
    "        try:\n",
    "            df = pd.read_csv(filepath)\n",
    "            viz_data[key] = df\n",
    "            print(f\"✅ Loaded {key}: {len(df):,} records\")\n",
    "        except FileNotFoundError:\n",
    "            print(f\"❌ File not found: {filepath}\")\n",
    "            viz_data[key] = pd.DataFrame()\n",
    "        except Exception as e:\n",
    "            print(f\"❌ Error loading {key}: {e}\")\n",
    "            viz_data[key] = pd.DataFrame()\n",
    "    \n",
    "    return viz_data\n",
    "\n",
    "# Load data\n",
    "print(\"📁 Loading visualization data...\")\n",
    "viz_data = load_visualization_data()\n",
    "\n",
    "# Data preparation\n",
    "if not viz_data['vehicles'].empty:\n",
    "    # Add derived features for visualization\n",
    "    df = viz_data['vehicles'].copy()\n",
    "    \n",
    "    # Speed categories\n",
    "    df['speed_category'] = pd.cut(df['speed'], \n",
    "                                 bins=[0, 10, 20, 30, 50], \n",
    "                                 labels=['Slow', 'Medium', 'Fast', 'Very Fast'])\n",
    "    \n",
    "    # Risk level based on speed and acceleration\n",
    "    df['risk_level'] = 'Low'\n",
    "    df.loc[(df['speed'] > 25) | (abs(df['acceleration']) > 2), 'risk_level'] = 'Medium'\n",
    "    df.loc[(df['speed'] > 30) | (abs(df['acceleration']) > 3), 'risk_level'] = 'High'\n",
    "    \n",
    "    # Vehicle type for coloring\n",
    "    df['vehicle_type'] = df['is_ego'].map({True: 'Ego Vehicle', False: 'Other Vehicle'})\n",
    "    \n",
    "    viz_data['vehicles_enhanced'] = df\n",
    "    \n",
    "    print(f\"\\n📊 Data prepared for visualization\")\n",
    "    print(f\"Time range: {df['timestamp'].min():.1f}s - {df['timestamp'].max():.1f}s\")\n",
    "    print(f\"Speed range: {df['speed'].min():.1f} - {df['speed'].max():.1f} m/s\")\nelse:\n",
    "    print(\"⚠️  No vehicle data available for visualization\")"
   ]
  },
